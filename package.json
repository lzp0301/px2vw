{
    "name": "px2vw",
    "displayName": "px2vw",
    "description": "一个px值转vw的VSCode插件",
    "version": "0.0.1",
    "publisher": "Roc",
    "icon": "",
    "license": "MIT",
    "licenseUrl": "LICENSE",
    "engines": {
        "vscode": "^1.12.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lzp0301/px2vw.git"
    },
    "bugs": {
        "url": "https://github.com/lzp0301/px2vw/issues",
        "email": "recodelife0301@gmail.com"
    },
    "homepage": "https://github.com/lzp0301/px2vw/",
    "categories": [
        "Formatters",
        "Snippets",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:html",
        "onLanguage:vue",
        "onLanguage:css",
        "onLanguage:less",
        "onLanguage:scss",
        "onLanguage:sass",
        "onLanguage:stylus"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.px2vw",
                "title": "px2vw px2Vw"
            }
        ],
        "keybindings": [
            {
                "command": "extension.px2vw",
                "key": "ctrl+p ctrl+r"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "px2vw 配置",
            "properties": {
                "px2vw.baseWidth": {
                    "type": "number",
                    "default": 375,
                    "description": "base window width (unit: px), default: 375"
                },
                "px2vw.fixedDigits": {
                    "type": "number",
                    "default": 6,
                    "description": "px转rem小数点最大长度，默认：6。"
                },
                "px2vw.autoRemovePrefixZero": {
                    "type": "boolean",
                    "default": true,
                    "description": "自动移除0开头的前缀，默认：true"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "package": "vsce package"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^10.1.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
